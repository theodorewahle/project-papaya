    "react-redux": "^5.0.7",
    "redux": "^4.0.0",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "eslint-config-prettier": "^2.9.0",
    "eslint-config-universe": "^1.0.7",
    "eslint-plugin-react": "^7.10.0",
